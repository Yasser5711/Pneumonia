{
  "name": "server",
  "packageManager": "yarn@4.3.1",
  "scripts": {
    "dev": "nodemon --watch src -e ts,js --exec \"tsx src/index.ts\" --legacy-watch",
    "test:predict": "tsx test/test.ts",
    "build": "tsc",
    "check:lint": "eslint . --ext .ts && prettier --check \"src/**/*.{ts,js,json,md,yml,yaml}\"",
    "check:types": "tsc -p tsconfig.typecheck.json",
    "fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "check:knip": "knip",
    "check:dep": "depcheck",
    "check:prune": "ts-prune ./src",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "check:test": "vitest run --silent --coverage",
    "check:all": "yarn check:lint && yarn check:types && yarn check:knip && yarn check:dep && yarn check:prune"
  },
  "type": "module",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/rate-limit": "^10.2.2",
    "@trpc/client": "^11.1.0",
    "@trpc/server": "^11.1.0",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.0",
    "nodemon": "^3.1.9",
    "sharp": "^0.34.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@fastify/websocket": "^11.0.2",
    "@types/node": "^22.14.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/ui": "^3.1.1",
    "depcheck": "^1.4.7",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "knip": "^5.50.2",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "ts-prune": "^0.10.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  }
}
