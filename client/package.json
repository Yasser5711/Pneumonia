{
  "name": "client",
  "private": true,
  "version": "4.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --build --force && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.ts --fix",
    "format:eslint": "eslint . --ext .vue,.ts,.js --fix",
    "format:stylelint": "stylelint \"src/**/*.{vue,css,scss,postcss}\" --fix",
    "format:prettier": "prettier --write \"src/**/*.{vue,ts,js,json,css,scss,md}\"",
    "format": "yarn format:eslint && yarn format:stylelint && yarn format:prettier",
    "lint:css": "stylelint \"src/**/*.{vue,css,scss,postcss}\" --fix",
    "check:lint": "eslint . --ext .vue,.js,.ts --fix && prettier --check 'src/**/*.{ts,js,vue,json,yml,yaml}' && stylelint \"src/**/*.{vue,css,scss,postcss}\" && echo 🚨✅ Lint check passed!",
    "check:types": "vue-tsc --noEmit && echo 🧠✅ Type check passed!",
    "check:build": "vite build && echo 🏗️✅ Build passed!",
    "check:pwa": "vite build && echo ⚙️✅ PWA build passed!",
    "check:test": "vitest run --coverage && echo 🧪✅ Tests passed! || (echo '❌ Fix the above issues before committing 💡' && exit 1)",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "check:knip": "knip && echo ✂️✅ Unused code check passed!",
    "check:dep": "depcheck && echo 📦✅ Dependency check passed!",
    "check:prune": "ts-prune -e ./src && echo 🌿✅ Prune check passed!",
    "check:all": "yarn check:lint && yarn check:types && yarn check:build && yarn check:pwa && yarn check:knip && yarn check:dep && yarn check:prune && echo 🧰✨ All checks passed! || (echo '❌ Fix the above issues before committing 💡' && exit 1)",
    "extract:front": "node test.js"
  },
  "dependencies": {
    "@duannx/vue-router-transition": "^1.3.0",
    "@mdi/font": "7.4.47",
    "@pixi/constants": "^7.4.3",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/vue-query": "^5.77.2",
    "@trpc/client": "^11.4.4",
    "@unhead/vue": "^2.0.10",
    "@vueuse/core": "^13.6.0",
    "@vueuse/motion": "^3.0.3",
    "better-auth": "^1.3.4",
    "core-js": "^3.42.0",
    "lucide-vue-next": "^0.511.0",
    "pinia": "^3.0.2",
    "pixi.js": "^8.11.0",
    "simplex-noise": "^4.0.3",
    "superjson": "^2.2.2",
    "tweakpane": "^4.0.5",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuetify": "^3.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tanstack/vue-query-devtools": "^5.83.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@tweakpane/core": "^2.0.5",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "knip": "^5.62.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "prettier-plugin-sort-json": "^4.1.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "1.90.0",
    "sass-embedded": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.1.12",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.2",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.5"
  },
  "packageManager": "yarn@4.9.2"
}
