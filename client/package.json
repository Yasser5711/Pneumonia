{
  "name": "client",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wireit",
    "client:dev": "vite",
    "build": "vue-tsc --build --force & vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.ts --fix",
    "format": "prettier --write 'src/**/*.{vue,ts,js,json,css,scss,md}'",
    "lint:css": "stylelint \"src/**/*.{vue,css,scss,postcss}\" --fix",
    "check:lint": "eslint . --ext .vue,.js,.ts --fix && prettier --check 'src/**/*.{ts,js,vue,json,yml,yaml}' && stylelint \"src/**/*.{vue,css,scss,postcss}\" && echo 🚨✅ Lint check passed!",
    "check:types": "vue-tsc --noEmit && echo 🧠✅ Type check passed!",
    "check:build": "vite build && echo 🏗️✅ Build passed!",
    "check:pwa": "vite build && echo ⚙️✅ PWA build passed!",
    "check:test": "vitest run --coverage && echo 🧪✅ Tests passed! || (echo '❌ Fix the above issues before committing 💡' && exit 1)",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "check:knip": "knip && echo ✂️✅ Unused code check passed!",
    "check:dep": "depcheck && echo 📦✅ Dependency check passed!",
    "check:prune": "ts-prune -e ./src && echo 🌿✅ Prune check passed!",
    "check:all": "yarn check:lint && yarn check:types && yarn check:build && yarn check:pwa && yarn check:knip && yarn check:dep && yarn check:prune && echo 🧰✨ All checks passed! || (echo '❌ Fix the above issues before committing 💡' && exit 1)",
    "extract:front": "node test.js"
  },
  "wireit": {
    "dev": {
      "command": "concurrently --names \"client\" --prefix-colors \"magenta\" \"vite\"",
      "service": true
    }
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@tanstack/vue-query": "^5.73.3",
    "@trpc/client": "^11.1.0",
    "@trpc/server": "^11.1.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/motion": "^3.0.3",
    "core-js": "^3.41.0",
    "lucide-vue-next": "^0.487.0",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-final-modal": "^4.5.5",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.4",
    "knip": "^5.50.5",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "prettier-plugin-sort-json": "^4.1.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "1.86.3",
    "sass-embedded": "^1.86.3",
    "stylelint": "^16.18.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "3.3.6",
    "ts-prune": "^0.10.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.6",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.1.1",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "yarn@4.3.1"
}
