{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "format": "prettier --write 'src/**/*.{vue,ts,js,json,css,scss,md}'",
    "lint:css": "stylelint \"src/**/*.{vue,css,scss,postcss}\" --fix",
    "extract:front": "node test.js",
    "check:lint": "eslint . --ext .vue,.js,.ts --fix && prettier --check 'src/**/*.{ts,js,vue,json,md,yml,yaml}' && stylelint 'src/**/*.{vue,css,scss,postcss}'",
    "check:types": "vue-tsc --noEmit",
    "check:build": "vite build",
    "check:pwa": "vite build && echo 'âœ… PWA build passed'",
    "check:test": "vitest run --silent --coverage",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "check:knip": "knip",
    "check:dep": "depcheck",
    "check:prune": "ts-prune ./src",
    "check:all": "yarn check:lint && yarn check:types && yarn check:build && yarn check:pwa && yarn check:knip && yarn check:dep && yarn check:prune"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@mdi/font": "7.4.47",
    "@tanstack/vue-query": "^5.73.3",
    "@trpc/client": "^11.1.0",
    "@trpc/server": "^11.1.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/motion": "^3.0.3",
    "core-js": "^3.41.0",
    "lucide-vue-next": "^0.487.0",
    "medium-zoom": "^1.1.0",
    "pinia": "^3.0.2",
    "roboto-fontface": "*",
    "vue": "^3.5.13",
    "vue-final-modal": "^4.5.5",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/ui": "^3.1.1",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "happy-dom": "^17.4.4",
    "jsdom": "^26.1.0",
    "knip": "^5.50.2",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "1.86.3",
    "sass-embedded": "^1.86.3",
    "stylelint": "^16.18.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "3.3.6",
    "ts-prune": "^0.10.3",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.6",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.1.1",
    "vue-test-utils": "^1.0.0-beta.11",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "yarn@4.7.0"
}
